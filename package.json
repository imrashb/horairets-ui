{
  "name": "horairets-ui",
  "version": "1.0.0",
  "type": "module",
  "description": "HorairÉTS est un générateur de combinaisons d'horaire pour l'École de technologie supérieure",
  "license": "MIT",
  "author": "Emmanuel Coulombe <emmanuel.coulombe.1@ens.etsmtl.ca>",
  "keywords": [
    "react",
    "javaScript",
    "vite",
    "material-ui",
    "eslint",
    "HorairÉTS"
  ],
  "scripts": {
    "dev": "vite --open",
    "firebase:setup": "npm install -g firebase-tools && firebase logout && firebase login && firebase init emulators && cd functions && npm i",
    "firebase:start": "firebase emulators:start",
    "start": "vite --open",
    "host": "vite --open --host",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "test:e2e": "yarn preview:test 'cypress open'",
    "test:e2e:headless": "yarn preview:test 'cypress run'",
    "test:e2e:ci": "vite build && yarn preview:test 'cypress run --record'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.10.17",
    "@mui/system": "^5.11.0",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.2.1",
    "classnames": "^2.3.2",
    "firebase": "^9.19.1",
    "framer-motion": "^12.25.0",
    "i18next": "^22.4.5",
    "i18next-http-backend": "^2.1.0",
    "jotai": "^2.16.0",
    "js-file-download": "^0.4.12",
    "lodash": "^4.17.21",
    "randomcolor": "^0.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-i18next": "^12.1.1",
    "react-icons": "^4.7.1",
    "react-router-dom": "^7.12.0",
    "react-toastify": "^9.1.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.0.3",
    "@types/randomcolor": "^0.5.9",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.36",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-istanbul": "0.25.6",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.25.6"
  },
  "resolutions": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  }
}
